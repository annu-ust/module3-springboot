package com.ust.iocautowiring;

public class B {
	
	A a;

	//public A getA() {
	//	return a;
	//}

	public B(A a) {
		super();
		this.a = a;
		System.out.println("from B() constructor....");
	}

	public void setA(A a) {
		//by name ,by type will use setter injection
		System.out.println("setA() called....");
		this.a = a;
	}



}
